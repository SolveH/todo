,IAAA;AACA;AAvBA,GAfA;AAwCA,EAAA,OAxCA,qBAwCA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,qBACA,MAAA,CAAA,SAAA,EADA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAEA;AA1CA,CAAA","sourcesContent":["<template>\n  <div>\n    <TodoItem\n      v-for=\"todo in todos\"\n      :key=\"todo.id\"\n      :todo=\"todo\"\n      @deleteTodoById=\"deleteTodo\"\n      @editTodoById=\"editTodo\"\n    />\n    <input type=\"text\" name=\"todoText\" v-model=\"inputText\" v-on:keyup.enter=\"addTodo\"/>\n    <md-button class=\"md-raised md-primary\" @click=\"addTodo\">Add todo</md-button>\n  </div>\n</template>\n\n<script>\nimport TodoItem from \"@/components/TodoItem\";\nimport { v4 as uuidv4 } from \"uuid\";\n\nexport default {\n  name: \"TodoList\",\n  computed: {\n    todos() {\n      return this.$store.getters.getTodos;\n    }\n  },\n  data() {\n    return {\n      inputText: \"\"\n    };\n  },\n  components: {\n    TodoItem\n  },\n  methods: {\n    addTodo() {\n      if (this.inputText != \"\") {\n        let randomId = uuidv4();\n        const newTodo = {\n          id: randomId,\n          name: this.inputText,\n          complete: false\n        };\n        this.$store.dispatch(\"addTodo\", newTodo);\n        this.inputText = \"\";\n      } else {\n        alert(\"Please write a todo text!\");\n      }\n    },\n    async loadTodos() {\n      await this.$store.dispatch(\"getTodos\");\n    },\n    deleteTodo(todo) {\n      this.$store.dispatch(\"deleteTodo\", todo);\n    },\n    editTodo(todo) {\n      this.$store.dispatch(\"editTodo\", todo);\n    }\n  },\n  async created() {\n    await this.loadTodos();\n  }\n};\n</script>\n"],"sourceRoot":"src/components"}]}